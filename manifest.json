{
  "name": "Darkmode",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Darkmode in just a few lines",
  "content_scripts": [
    {
      "matches": [
        "*://golang.org/*",
        "*://godoc.org/*",
        "*://slides.google.com/*",
        "*://web.whatsapp.com/*",
        "*://web.telegram.org/*",
        "*://twitter.com/*"
      ], 
      "css": ["darkmode.css"]
    }
  ],
  "permissions": [ "tabs", "*://godoc.org/*" ]
}
