{
    "name": "Darkmode",
    "version": "0.0.2",
    "manifest_version": 2,
    "description": "Darkmode in just a few lines",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "css": [
                "darkmode.css"
            ]
        }
    ],
    "browser_action": {},
    "permissions": [
        "tabs",
        "activeTab",
        "*://*/*"
    ]
}